doctype html
html(lang="en")
  include ./partials/head.pug
  body.kt-quick-panel--right.kt-demo-panel--right.kt-offcanvas-panel--right.kt-header--fixed.kt-header-mobile--fixed.kt-subheader--enabled.kt-subheader--fixed.kt-subheader--solid.kt-aside--enabled.kt-aside--fixed.kt-page--loading.overlayloader
    // begin:: Page
    // begin:: Header Mobile
    #kt_header_mobile.kt-header-mobile.kt-header-mobile--fixed
      .kt-header-mobile__logo
        a(href="index.html")
          img(alt="Logo" src="/assets/media/logos/logo-dark.png")
      .kt-header-mobile__toolbar
        button#kt_aside_mobile_toggler.kt-header-mobile__toggler.kt-header-mobile__toggler--left
          span
        button#kt_header_mobile_toggler.kt-header-mobile__toggler
          span
        button#kt_header_mobile_topbar_toggler.kt-header-mobile__topbar-toggler
          i.flaticon-more
    // end:: Header Mobile
    .kt-grid.kt-grid--hor.kt-grid--root
      .kt-grid__item.kt-grid__item--fluid.kt-grid.kt-grid--ver.kt-page
        include ./partials/sidenav.pug
        #kt_wrapper.kt-grid__item.kt-grid__item--fluid.kt-grid.kt-grid--hor.kt-wrapper
          // begin:: Header
          include ./partials/topnav.pug
          #kt_content.kt-content.kt-grid__item.kt-grid__item--fluid.kt-grid.kt-grid--hor
            // begin:: Content Head
            include ./partials/header.pug
            .kt-container.kt-container--fluid.kt-grid__item.kt-grid__item--fluid
              // Begin::Dashboard 3
              // Begin::Row
              .kt-portlet
                .kt-portlet__body
                  .kt-widget.kt-widget--user-profile-3
                    .kt-widget__top
                      //.kt-widget__pic.kt-widget__pic--danger.kt-font-danger.kt-font-bolder.kt-font-light#clientinitials
                      .kt-widget__content
                        .kt-widget__head
                          .kt-widget__user
                            a.kt-widget__username#clientname(href='#')
                            span.kt-badge.kt-badge--bolder.kt-badge.kt-badge--inline.kt-badge--unified-success#emprole
                          .kt-widget__action
                            a#tillEot.btn.kt-subheader__btn-secondary(href='javascript:void(0);' onclick="tillEOT()") Till EOT
                            a#kt_dashboard_daterangepicker_3.btn.kt-subheader__btn-daterange(href='javascript:void(0);', data-toggle='kt-tooltip', title='Select dashboard daterange', data-placement='left')
                              span#kt_dashboard_daterangepicker_title_3.kt-subheader__btn-daterange-title(style="padding-right:0.6rem") Current 
                              span#kt_dashboard_daterangepicker_date_3.kt-subheader__btn-daterange-date  01-2020
                              i.flaticon2-calendar-1
                        //.kt-widget__subhead
                          a(href='#')#clientemail
                          a(href='#')#clientphone
                          a(href='#')#joiningdate
                        //.kt-widget__info
                          .kt-widget__progress
                            .kt-widget__text
                              | Progress
                            .progress(style='height: 5px;width: 100%;')
                              .progress-bar.progress-bar-striped.progress-bar-animated.kt-bg-success(role='progressbar', style='width: 65%;', aria-valuenow='65', aria-valuemin='0', aria-valuemax='100')
                            .kt-widget__stats
                              | 78%
                    .kt-widget__bottom
                      .kt-widget__item
                        .kt-widget__icon
                          i.flaticon-confetti
                        .kt-widget__details
                          span.kt-widget__title Total task
                          span.kt-widget__value
                            span#totaltask
                      //.kt-widget__item#ttotal
                        .kt-widget__icon
                          i.flaticon-pie-chart
                        .kt-widget__details
                          span.kt-widget__title Total wordcount
                          span.kt-widget__value
                            span#totalwordcount
                      .kt-widget__item
                        .kt-widget__icon
                          i.flaticon2-writing
                        .kt-widget__details
                          span.kt-widget__title Achived Wordcount
                          span.kt-widget__value
                            span#achivedwordcount
              .kt-portlet.kt-portlet--mobile
                .kt-portlet__head
                  .kt-portlet__head-label
                    h3.kt-portlet__head-title
                      | Effort sheet
                  .kt-portlet__head-toolbar
                    .kt-portlet__head-toolbar-wrapper
                      button#kt_datatable_reload.btn.btn-outline-brand.btn-sm(type='button', style="margin-right:0.5rem;", data-toggle='dropdown', aria-haspopup='true', aria-expanded='false')
                        i.la.la-refresh
                        |  Reload
                      .dropdown.dropdown-inline
                        button.btn.btn-outline-brand.btn-sm(type='button', style="margin-right:0.5rem;", data-toggle='dropdown', aria-haspopup='true', aria-expanded='false')
                          i.la.la-download
                          |  Export
                        button.btn.btn-brand.btn-sm(type='button', data-toggle='modal', data-target='#kt_modal_4' onclick="clearForm()")
                          i.la.la-plus
                          |  Add Task                            
                        .dropdown-menu.dropdown-menu-right
                          ul.kt-nav
                            li.kt-nav__section.kt-nav__section--first
                              span.kt-nav__section-text Export Tools
                            li.kt-nav__item
                              a#export_print.kt-nav__link(href='#')
                                i.kt-nav__link-icon.la.la-print
                                span.kt-nav__link-text Print
                            li.kt-nav__item
                              a#export_copy.kt-nav__link(href='#')
                                i.kt-nav__link-icon.la.la-copy
                                span.kt-nav__link-text Copy
                            li.kt-nav__item
                              a#export_excel.kt-nav__link(href='#')
                                i.kt-nav__link-icon.la.la-file-excel-o
                                span.kt-nav__link-text Excel
                            li.kt-nav__item
                              a#export_csv.kt-nav__link(href='#')
                                i.kt-nav__link-icon.la.la-file-text-o
                                span.kt-nav__link-text CSV
                            li.kt-nav__item
                              a#export_pdf.kt-nav__link(href='#')
                                i.kt-nav__link-icon.la.la-file-pdf-o
                                span.kt-nav__link-text PDF
                #addTable.kt-portlet__body(style="padding-top:0")
                  .kt-form.kt-form--label-right.kt-margin-t-20.kt-margin-b-10
                      .row.align-items-center
                        .col-xl-12.order-2.order-xl-1
                          .row.align-items-center
                            .col-md-4.kt-margin-b-20-tablet-and-mobile
                              .kt-input-icon.kt-input-icon--left
                                input#generalSearch.form-control(type='text', placeholder='Search...')
                                span.kt-input-icon__icon.kt-input-icon__icon--left
                                  span
                                    i.la.la-search
                  // begin: Datatable
                  table#kt_table_3.table.table-striped-.table-bordered.table-hover.table-checkable
                    thead
                      tr
                        th Key
                        th Title
                        th Wordcount
                        th Completed on
                        th Comments
                        th Status
                    tfoot
                      tr
                        th(colspan="2") Total:
                        th(colspan="4")
          include ./partials/footer.pug
          // begin::Modal
          #kt_modal_4.modal.fade(tabindex='-1', role='dialog', aria-labelledby='exampleModalLabel', aria-hidden='true')
            .modal-dialog.modal-lg(role='document')
              .modal-content
                .modal-header
                  h5#modalTitle.modal-title Add your task
                  button.close(type='button', data-dismiss='modal', aria-label='Close' onclick="closeModal()")
                .modal-body(class="kt-scroll" data-scroll="true")
                  form#createEffortsheet1
                    label.form-control-label#effortformerror(for='email' style="color:red;")
                    .form-group
                      label.form-control-label(for='email') Task Key:
                      input#taskidnew.form-control(type='text' name="email")
                      label.form-control-label#taskkeyerror(for='email' style="color:red;")
                      br
                      button#validateButton.btn.btn-primary(type='button' onclick="validateTask()" style="margin-top:10px;") 
                        i.la.la-random
                        |  Validate
                    .form-group#Taskvalidated(style="display:none;")
                      .form-group
                        label.form-control-label(for='email') Title:
                        input#titlenew.form-control(type='text' name="email")
                      .form-group
                        label.form-control-label(for='name') Achieved wordcount:
                        input#achived_wordcountnew.form-control(type='number' name="name")
                      .form-group
                        label.form-control-label(for='phone') Submitted on:
                        input#submittedonnew.form-control(type='date' name="phone")
                      .form-group
                        label.form-control-label(for='phone') Comments:
                        input#commentforeffort.form-control(type='text' name="comment")
                .modal-footer
                  button.btn.btn-secondary(type='button', data-dismiss='modal' onclick="closeModal()") Close
                  button#actionButton.btn.btn-primary(type='button' onclick="createEffort()") Add
          // end::Modal
    #kt_scrolltop.kt-scrolltop
      i.fa.fa-arrow-up
