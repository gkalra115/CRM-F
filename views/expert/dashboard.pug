doctype html
html(lang="en")
  include ./partials/head.pug
  body.kt-quick-panel--right.kt-demo-panel--right.kt-offcanvas-panel--right.kt-header--fixed.kt-header-mobile--fixed.kt-subheader--enabled.kt-subheader--fixed.kt-subheader--solid.kt-aside--enabled.kt-aside--fixed.kt-page--loading.overlayloader
    // begin:: Page
    // begin:: Header Mobile
    #kt_header_mobile.kt-header-mobile.kt-header-mobile--fixed
      .kt-header-mobile__logo
        a(href="index.html")
          img(alt="Logo" src="/assets/media/logos/logo-dark.png")
      .kt-header-mobile__toolbar
        button#kt_aside_mobile_toggler.kt-header-mobile__toggler.kt-header-mobile__toggler--left
          span
        button#kt_header_mobile_toggler.kt-header-mobile__toggler
          span
        button#kt_header_mobile_topbar_toggler.kt-header-mobile__topbar-toggler
          i.flaticon-more
    // end:: Header Mobile
    .kt-grid.kt-grid--hor.kt-grid--root
      .kt-grid__item.kt-grid__item--fluid.kt-grid.kt-grid--ver.kt-page
        include ./partials/sidenav.pug
        #kt_wrapper.kt-grid__item.kt-grid__item--fluid.kt-grid.kt-grid--hor.kt-wrapper
          // begin:: Header
          include ./partials/topnav.pug
          #kt_content.kt-content.kt-grid__item.kt-grid__item--fluid.kt-grid.kt-grid--hor
            // begin:: Content Head
            include ./partials/header.pug
            .kt-container.kt-container--fluid.kt-grid__item.kt-grid__item--fluid
              // Begin::Dashboard 3
              // Begin::Row
              .kt-portlet
                .kt-portlet__body.kt-portlet__body--fit
                  .row.row-no-padding.row-col-separator-lg
                    .col-md-12.col-lg-4.col-xl-4
                      // begin::Total Profit
                      .kt-widget24
                        .kt-widget24__details
                          .kt-widget24__info
                            h4.kt-widget24__title
                              | Total Task
                            a(href='/ex/all')
                              span.kt-widget24__desc
                                | Click here for all task
                          span.kt-widget24__stats.kt-font-danger
                              span.kt-spinner.kt-spinner--sm.kt-spinner--danger.demo(style="margin-right : 20px")
                              span#totaltask
                    .col-md-12.col-lg-4.col-xl-4
                      // begin::New Feedbacks
                      .kt-widget24
                        .kt-widget24__details
                          .kt-widget24__info
                            h4.kt-widget24__title
                              | Delayed Task
                            a(href='#addTable')
                              span.kt-widget24__desc
                                | Click here for all delayed task
                          span.kt-widget24__stats.kt-font-success
                              span.kt-spinner.kt-spinner--sm.kt-spinner--success.demo(style="margin-right : 20px")
                              span#delayedTaskCount
                    .col-md-12.col-lg-4.col-xl-4
                      // begin::New Feedbacks
                      .kt-widget24
                        .kt-widget24__details
                          .kt-widget24__info
                            h4.kt-widget24__title
                              | Running Task
                            a(href='/ex/task')
                              span.kt-widget24__desc
                                | Click here for all task in progress
                          span.kt-widget24__stats.kt-font-warning
                            //span.kt-spinner.kt-spinner--sm.kt-spinner--warning.demo(style="margin-right : 20px")
                            span#currenttask
                div
                div
                div
                div
              .kt-portlet.kt-portlet--mobile
                .kt-portlet__head
                  .kt-portlet__head-label
                    h3.kt-portlet__head-title
                      | Delayed task
                #addTable.kt-portlet__body(style="padding-top:0")
                  .kt-form.kt-form--label-right.kt-margin-t-20.kt-margin-b-10
                      .row.align-items-center
                        .col-xl-12.order-2.order-xl-1
                          .row.align-items-center
                            .col-md-4.kt-margin-b-20-tablet-and-mobile
                              .kt-input-icon.kt-input-icon--left
                                input#generalSearch.form-control(type='text', placeholder='Search...')
                                span.kt-input-icon__icon.kt-input-icon__icon--left
                                  span
                                    i.la.la-search
                  // begin: Datatable
                  table#kt_table_3.table.table-striped-.table-bordered.table-hover.table-checkable
                    thead
                      tr
                        th Key
                        th Title
                        th Deadline
                        th Status
                        th Lead
          include ./partials/footer.pug
    #kt_scrolltop.kt-scrolltop
      i.fa.fa-arrow-up
    script.
      var KTAppOptions = {
