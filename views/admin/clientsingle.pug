doctype html
html(lang="en")
  include ./partials/head.pug
  body.kt-quick-panel--right.kt-demo-panel--right.kt-offcanvas-panel--right.kt-header--fixed.kt-header-mobile--fixed.kt-subheader--enabled.kt-subheader--fixed.kt-subheader--solid.kt-aside--enabled.kt-aside--fixed.kt-page--loading
    // begin:: Page
    // begin:: Header Mobile
    #kt_header_mobile.kt-header-mobile.kt-header-mobile--fixed
      .kt-header-mobile__logo
        a(href="index.html")
          img(alt="Logo" src="/assets/media/logos/logo-dark.png")
      .kt-header-mobile__toolbar
        button#kt_aside_mobile_toggler.kt-header-mobile__toggler.kt-header-mobile__toggler--left
          span
        button#kt_header_mobile_toggler.kt-header-mobile__toggler
          span
        button#kt_header_mobile_topbar_toggler.kt-header-mobile__topbar-toggler
          i.flaticon-more
    // end:: Header Mobile
    .kt-grid.kt-grid--hor.kt-grid--root
      .kt-grid__item.kt-grid__item--fluid.kt-grid.kt-grid--ver.kt-page
        include ./partials/sidenav.pug
        #kt_wrapper.kt-grid__item.kt-grid__item--fluid.kt-grid.kt-grid--hor.kt-wrapper
          // begin:: Header
          include ./partials/topnav.pug
          #kt_content.kt-content.kt-grid__item.kt-grid__item--fluid.kt-grid.kt-grid--hor
            // begin:: Content Head
            include ./partials/header.pug
            .kt-grid.kt-grid--ver.kt-grid--root#errorclient(style="display:none;")
              .kt-grid__item.kt-grid__item--fluid.kt-grid.kt-error-v5(style='background-image: url(/assets/media/error/bg5.jpg);')
                .kt-error_container
                  span.kt-error_title
                    h1 Oops!
                  p.kt-error_subtitle
                    | Something went wrong here.
                  p.kt-error_description
                    | There is no such Client
            .kt-container.kt-container--fluid.kt-grid__item.kt-grid__item--fluid#clientportlet(style="display:none")
              // Begin::Dashboard 3
              // Begin::Row
              .kt-portlet
                .kt-portlet__body
                  .kt-widget.kt-widget--user-profile-3
                    .kt-widget__top
                      .kt-widget__pic.kt-widget__pic--danger.kt-font-danger.kt-font-bolder.kt-font-light#clientinitials
                      .kt-widget__content
                        .kt-widget__head
                          .kt-widget__user
                            a.kt-widget__username#clientname(href='#')
                              | David Smith
                            span.kt-badge.kt-badge--bolder.kt-badge.kt-badge--inline.kt-badge--unified-success Client
                          .kt-widget__action
                            button.btn.btn-outline-hover-success#whatsapp(type='button')
                              i.flaticon-whatsapp
                              |  Whatsapp
                            a#kt_dashboard_daterangepicker_3.btn.kt-subheader__btn-daterange(href='javascript:void(0);', data-toggle='kt-tooltip', title='Select dashboard daterange', data-placement='left')
                              span#kt_dashboard_daterangepicker_title_3.kt-subheader__btn-daterange-title(style="padding-right:0.6rem") Current 
                              span#kt_dashboard_daterangepicker_date_3.kt-subheader__btn-daterange-date  01-2020
                              i.flaticon2-calendar-1
                        .kt-widget__subhead
                          a(href='#')#clientemail
                          a(href='#')#clientphone
                          a(href='#')#clientuni
                          a(href='#')#clientcountry
                    .kt-widget__bottom
                      .kt-widget__item
                        .kt-widget__icon
                          i.flaticon-piggy-bank
                        .kt-widget__details
                          span.kt-widget__title Revenue
                          span.kt-widget__value
                            span#currencyrevenue
                      .kt-widget__item
                        .kt-widget__icon
                          i.flaticon-confetti
                        .kt-widget__details
                          span.kt-widget__title Paid
                          span.kt-widget__value
                            span#currencypaid
                      .kt-widget__item
                        .kt-widget__icon
                          i.flaticon-pie-chart
                        .kt-widget__details
                          span.kt-widget__title Total task
                          span.kt-widget__value
                            span
                            #totaltasklen
                      .kt-widget__item
                        .kt-widget__icon
                          i.flaticon2-writing
                        .kt-widget__details
                          span.kt-widget__title Wordcount
                          span.kt-widget__value
                            span
                            #totalwordcount
              .kt-portlet.kt-portlet--mobile
                .kt-portlet__head
                  .kt-portlet__head-label
                    h3.kt-portlet__head-title
                      | Client
                  .kt-portlet__head-toolbar
                    .kt-portlet__head-toolbar-wrapper
                      .dropdown.dropdown-inline
                        button.btn.btn-outline-brand.btn-sm(type='button', style="margin-right:0.5rem;", data-toggle='dropdown', aria-haspopup='true', aria-expanded='false')
                          i.la.la-download
                          |  Export
                        button.btn.btn-brand.btn-sm(type='button', data-toggle='modal', data-target='#kt_modal_4' onclick="clearForm()")
                          i.la.la-plus
                          |  Add Client                            
                        .dropdown-menu.dropdown-menu-right
                          ul.kt-nav
                            li.kt-nav__section.kt-nav__section--first
                              span.kt-nav__section-text Export Tools
                            li.kt-nav__item
                              a#export_print.kt-nav__link(href='#')
                                i.kt-nav__link-icon.la.la-print
                                span.kt-nav__link-text Print
                            li.kt-nav__item
                              a#export_copy.kt-nav__link(href='#')
                                i.kt-nav__link-icon.la.la-copy
                                span.kt-nav__link-text Copy
                            li.kt-nav__item
                              a#export_excel.kt-nav__link(href='#')
                                i.kt-nav__link-icon.la.la-file-excel-o
                                span.kt-nav__link-text Excel
                            li.kt-nav__item
                              a#export_csv.kt-nav__link(href='#')
                                i.kt-nav__link-icon.la.la-file-text-o
                                span.kt-nav__link-text CSV
                            li.kt-nav__item
                              a#export_pdf.kt-nav__link(href='#')
                                i.kt-nav__link-icon.la.la-file-pdf-o
                                span.kt-nav__link-text PDF
                .kt-portlet__body
                  // begin: Search Form
                  .kt-form.kt-form--label-right.kt-margin-t-20.kt-margin-b-10
                    .row.align-items-center
                      .col-xl-8.order-2.order-xl-1
                        .row.align-items-center
                          .col-md-4.kt-margin-b-20-tablet-and-mobile
                            .kt-input-icon.kt-input-icon--left
                              input#generalSearch.form-control(type='text', placeholder='Search...')
                              span.kt-input-icon__icon.kt-input-icon__icon--left
                                span
                                  i.la.la-search
                          .col-md-4.kt-margin-b-20-tablet-and-mobile
                            .kt-form__group.kt-form__group--inline
                              .kt-form__label
                                label Status:
                              .kt-form__control
                                select#kt_form_status.form-control.bootstrap-select
                                  option(value='') ----
                                  option(value='Active') Active
                                  option(value='In-Active') In-Active
                        .kt-separator.kt-separator--border-dashed.kt-separator--space-lg.d-xl-none
                  // end: Search Form
                #addTable.kt-portlet__body(style="padding-top:0")
                
                  // begin: Datatable
                  table#kt_table_3.table.table-striped-.table-bordered.table-hover.table-checkable
                    thead
                      tr
                        th Key
                        th Task Title
                        th Deadline
                        th Createdby
                        th Status
                        th Payment
                        th Cancelled
                  // end: Datatable
          include ./partials/footer.pug
          // end::Modal
          // begin::Modal
          
