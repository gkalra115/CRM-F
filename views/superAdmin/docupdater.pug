              // Begin::Row
              .row
                .col-md-12 
                  .kt-portlet.kt-portlet--mobile
                    .kt-portlet__head
                      .kt-portlet__head-label
                        h3.kt-portlet__head-title
                          | Update document properties
                    .kt-portlet__body(style="padding-top:0")
                      // begin: Datatable
                      form#createEffortsheet1
                        label.form-control-label#effortformerror(for='email' style="color:red;")
                        .form-group
                          label.form-control-label(for='email') Author name:
                          input#authorname.form-control(type='text' name="author")
                        .form-group
                          label.form-control-label(for='email') Modified by:
                          input#modifiedby.form-control(type='text' name="modifiedby")
                        .form-group
                          label.form-control-label(for='name') Created on:
                          input#createdon.form-control(type='date' name="createdon")
                        .form-group
                          label.form-control-label(for='phone') Modified on:
                          input#modifieddon.form-control(type='date' name="modifieddon")
                        .form-group
                          label.form-control-label(for='phone') File:
                          input#fileinput.form-control(type='file' name="file")
                        button.btn.btn-primary.kt-spinner--right.kt-spinner--md.kt-spinner--light#simpletospin(type='button' onclick="keywordextractor()")
                                | Update properties
                

