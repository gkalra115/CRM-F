                      label.form-control-label(for='email') Email:
                      input#email.form-control(type='email')
                    .form-group
                      label.form-control-label(for='text') Name:
                      input#name.form-control(type='text')
                    .form-group
                      label.form-control-label(for='phone') Phone:
                      input#phone.form-control(type='tel')
                    #statusInput.form-group(style="display:none;")
                      label(for='status') Status
                      select#status.form-control
                        option(value='Active') Active
                        option(value='In-Active') In-Active         
                .modal-footer
                  button.btn.btn-secondary(type='button', data-dismiss='modal' ) Close
                  button#actionButton.btn.btn-primary(type='button' onclick="createFreelancer()") Create
          // end::Modal
          // begin::Modal
          #kt_modal_3.modal.fade(tabindex='-1', role='dialog', aria-labelledby='editFreelancer', aria-hidden='true')
