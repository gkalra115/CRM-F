                      label.form-control-label(for='email') Email:
                      input#uemail.form-control(type='email')
                    .form-group
                      label.form-control-label(for='text') Name:
                      input#uname.form-control(type='text')
                    .form-group
                      label.form-control-label(for='phone') Phone:
                      input#uphone.form-control(type='tel')
                    .form-group
                      label(for='country') Country
                      select#ucountry.form-control
                        option(value='AU') AU
                        option(value='UK') UK
                        option(value='IN') IN
                        option(value='NZ') NZ
                        option(value='US') US
                        option(value='CN') CN
                    .form-group
                      label.form-control-label(for='university') University:
                      input#uuniversity.form-control(type='text')
                    .form-group
                      label(for='status') Status
                      select#ustatus.form-control
                        option(value='Active') Active
                        option(value='In-Active') In-Active
                        
                .modal-footer
                  button.btn.btn-secondary(type='button', data-dismiss='modal') Close
                  button.btn.btn-primary(type='button',id="editButton" onclick='editFreelancer()') Edit
          // end::Modal
