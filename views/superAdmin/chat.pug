doctype html
html(lang="en")
  include ./partials/head.pug
  link(href='https://cdn.jsdelivr.net/gh/gitbrent/bootstrap-switch-button@1.1.0/css/bootstrap-switch-button.min.css' rel='stylesheet')
  body.kt-quick-panel--right.kt-demo-panel--right.kt-offcanvas-panel--right.kt-header--fixed.kt-header-mobile--fixed.kt-subheader--enabled.kt-subheader--fixed.kt-subheader--solid.kt-aside--enabled.kt-aside--fixed.kt-page--loading
    // begin:: Page
    // begin:: Header Mobile
    #kt_header_mobile.kt-header-mobile.kt-header-mobile--fixed
      .kt-header-mobile__logo
        a(href="index.html")
          img(alt="Logo" src="/assets/media/logos/logo-dark.png")
      .kt-header-mobile__toolbar
        button#kt_aside_mobile_toggler.kt-header-mobile__toggler.kt-header-mobile__toggler--left
          span
        button#kt_header_mobile_toggler.kt-header-mobile__toggler
          span
        button#kt_header_mobile_topbar_toggler.kt-header-mobile__topbar-toggler
          i.flaticon-more
    // end:: Header Mobile
    .kt-grid.kt-grid--hor.kt-grid--root
      .kt-grid__item.kt-grid__item--fluid.kt-grid.kt-grid--ver.kt-page
        include ./partials/sidenav.pug
        #kt_wrapper.kt-grid__item.kt-grid__item--fluid.kt-grid.kt-grid--hor.kt-wrapper
          // begin:: Header
          include ./partials/topnav.pug
          #kt_content.kt-content.kt-grid__item.kt-grid__item--fluid.kt-grid.kt-grid--hor
            // begin:: Content Head
            include ./partials/header.pug
            .kt-container.kt-container--fluid.kt-grid__item.kt-grid__item--fluid
                #kt_content.kt-content.kt-grid__item.kt-grid__item--fluid.kt-grid.kt-grid--hor
                  // begin:: Content
                  .kt-container.kt-container--fluid.kt-grid__item.kt-grid__item--fluid
                    // Begin::App
                    .kt-grid.kt-grid--desktop.kt-grid--ver.kt-grid--ver-desktop.kt-app
                      #kt_chat_content.kt-grid__item.kt-grid__item--fluid.kt-app__content
                        .kt-chat
                          .kt-portlet.kt-portlet--head-lg.kt-portlet--last
                            .kt-portlet__head
                              .kt-chat__head
                                .kt-chat__center
                                  .kt-chat__label
                                    a.kt-chat__title(href='#') Oslo the Wise(beta v1.0)
                                    span.kt-chat__status
                                      span.kt-badge.kt-badge--dot.kt-badge--success
                                      |  Active
                            .kt-portlet__body
                              .kt-scroll.kt-scroll--pull(data-mobile-height='300')
                                .kt-chat__messages
                                  .kt-chat__message
                            .kt-portlet__foot
                              .kt-chat__input
                                .kt-chat__editor
                                  textarea(style='height: 50px' placeholder='Type here...')
                                .kt-chat__toolbar
                                  .kt_chat__actions
                                    button.btn.btn-brand.btn-md.btn-upper.btn-bold.kt-chat__reply(type='button') reply
          include ./partials/footer.pug
          // end::Modal
          // begin::Modal
          // end::Widget
    // end::Quick Panel
    #kt_scrolltop.kt-scrolltop
      i.fa.fa-arrow-up
