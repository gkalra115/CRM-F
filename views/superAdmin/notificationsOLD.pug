doctype html
html(lang="en")
  include ./partials/head.pug
  style.
      .removeNotfication {
          display : none ! important;
        }
  body.kt-quick-panel--right.kt-demo-panel--right.kt-offcanvas-panel--right.kt-header--fixed.kt-header-mobile--fixed.kt-subheader--enabled.kt-subheader--fixed.kt-subheader--solid.kt-aside--enabled.kt-aside--fixed.kt-page--loading.overlayloader
    // begin:: Page
    // begin:: Header Mobile
    #kt_header_mobile.kt-header-mobile.kt-header-mobile--fixed
      .kt-header-mobile__logo
        a(href="index.html")
          img(alt="Logo" src="/assets/media/logos/logo-dark.png")
      .kt-header-mobile__toolbar
        button#kt_aside_mobile_toggler.kt-header-mobile__toggler.kt-header-mobile__toggler--left
          span
        button#kt_header_mobile_toggler.kt-header-mobile__toggler
          span
        button#kt_header_mobile_topbar_toggler.kt-header-mobile__topbar-toggler
          i.flaticon-more
    // end:: Header Mobile
    .kt-grid.kt-grid--hor.kt-grid--root
      .kt-grid__item.kt-grid__item--fluid.kt-grid.kt-grid--ver.kt-page
        include ./partials/sidenav.pug
        #kt_wrapper.kt-grid__item.kt-grid__item--fluid.kt-grid.kt-grid--hor.kt-wrapper
          // begin:: Header
          include ./partials/topnav.pug
          #kt_content.kt-content.kt-grid__item.kt-grid__item--fluid.kt-grid.kt-grid--hor
            // begin:: Content Head
            include ./partials/header.pug
            .kt-container.kt-container--fluid.kt-grid__item.kt-grid__item--fluid
              // Begin::Dashboard 3
              // Begin::Row
              .row
              // begin:: Widgets/Trends
              .kt-portlet.kt-portlet--tabs.kt-portlet--height-fluid
                .kt-portlet__head
                  .kt-portlet__head-label
                    h3.kt-portlet__head-title
                      | Notifications
                .kt-portlet__body
                  // begin: Search Form
                  .kt-form.kt-form--label-right.kt-margin-t-20.kt-margin-b-10
                      .row.align-items-center
                        .col-xl-8.order-2.order-xl-1
                          .row.align-items-center
                            .col-md-4.kt-margin-b-20-tablet-and-mobile
                              .kt-input-icon.kt-input-icon--left
                              .input-group#generalSearch
                                input.form-control(type='text' placeholder='Search for...')
                                .input-group-append
                                  button.search.btn.btn-secondary(type='button') Go!

                                //- input#generalSearch.form-control(type='text', placeholder='Search...')
                                //- span.kt-input-icon__icon.kt-input-icon__icon--left
                                //-   span
                                //-     i.la.la-search
                            .col-md-4.kt-margin-b-20-tablet-and-mobile
                              .kt-form__group.kt-form__group--inline
                                .kt-form__label
                                  label Type:
                                .kt-form__control
                                  select#kt_form_type.form-control.bootstrap-select
                                    option(value='') All
                                    option(value='read') Read
                                    option(value='unread') Unread
                          .kt-separator.kt-separator--border-dashed.kt-separator--space-lg.d-xl-none
                  // end: Search Form

                .kt-portlet__body(style="padding-top:0")
                  // begin: Datatable
                  table#kt_table_3.table
                    thead(style="display:none")
                      tr
                        th title
                        th status
                  // end: Datatable

          include ./partials/footer.pug
    #kt_scrolltop.kt-scrolltop
      i.fa.fa-arrow-up
    script.
      var KTAppOptions = {
      "colors": {
      "state": {
      "brand": "#5d78ff",
      "dark": "#282a3c",
      "light": "#ffffff",
      "primary": "#5867dd",
      "success": "#34bfa3",
      "info": "#36a3f7",
      "warning": "#ffb822",
      "danger": "#fd3995"
      },
      "base": {
      "label": [
      "#c5cbe3",
      "#a1a8c3",
      "#3d4465",
      "#3e4466"
      ],
      "shape": [
      "#f0f3ff",
      "#d9dffa",
      "#afb4d4",
      "#646c9a"
      ]
      }
      }
      };
    // end::Global Config
    // begin::Global Theme Bundle(used by all pages)
    script(src="/assets/plugins/global/plugins.bundle.js" type="text/javascript")
    script(src="/assets/js/scripts.bundle.js" type="text/javascript")
    // end::Global Theme Bundle
    // begin::Page Vendors(used by this page)
    script(src="/assets/plugins/custom/fullcalendar/fullcalendar.bundle.js" type="text/javascript")
    script(src="/assets/plugins/custom/datatables/datatables.bundle.js" type="text/javascript")
